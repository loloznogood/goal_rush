{%  extends 'component/layout.html.twig' %}

{% block main %}

	<main role="main" class="container">
		{% include 'component/fil-ariane.html.twig' with data.filAriane %}

		<h1 class="text-center mt-5" id="title-div">
			NOS SERVICES</h1>
		<div class="row justify-content-center">
			{% for s in data.services %}
				<div class="col-md-4 m-5">
					<a href="{{ s.Url }}">
						<img id="intro-example" src="{{s.imageUrl}}" alt="image Service">
					</a>
					<div class="text-center p-2" id="servicedesc">
						<h3 class="">
							<a class="" id="test2" href="{{ s.Url }}">{{ s.intitule }}</a>
						</h3>
						<h5 class="">{{ s.description }}</h5>
					</div>
					<p class="text-center p-2" id="test">
						<a id="test2" href="{{ s.Url }}">En savoir plus</a>
					</p>
				</div>
			{% endfor %}
		</div>
	</main>
{% endblock %}
